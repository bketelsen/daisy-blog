---
import '../style/tailwind.css';

import Features from "@components/Features";
import Head from "@components/Head";
import MainLayout from "@components/MainLayout";
import "@fontsource/inter"

const settings = {}
const {title} = Astro.props;
const {desc} = Astro.props;
const {img} = Astro.props;

---

<!DOCTYPE html>
<html lang="en">

    <Head title={title} desc={desc} img={img} settings={settings}/>

<MainLayout settings={settings}>
  <Features title="Brian Ketelsen"
  subtitle="I'm a developer advocate, keynote speaker, author, open source practitioner. Welcome to my home on the web."
  tag="hello"
  slot="hero" />
  <slot />
</MainLayout>
</html>

